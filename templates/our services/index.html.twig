{% extends "base.html.twig" %}

{% block title %}Nos prestations
{% endblock %}

{% block body %}

    <div class="">

        {% for service in services %}
            <services>
                <h2>{{ service.title }}</h2>
                <img src="{{ service.picture }}" alt="image de la prestation">
                <div>{{ service.description | raw }}</div>
                <div>{{ service.price }}</div>
            </services>
        {% endfor %}

    </div>

{% endblock %}
